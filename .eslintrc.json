{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "class-methods-use-this": ["error", { "enforceForClassFields": false }],
        "import/no-dynamic-require": "off",
        "global-require": "off",
        "no-unused-vars": "off",
        "linebreak-style": 0,
        "no-useless-escape": "off",
        "no-return-await": "off",
        "no-self-assign": "off",
        "no-undef": "off",
        "no-unreachable": "off",
        "arrow-body-style": "off",
        "no-restricted-globals": "off"
    }
}
